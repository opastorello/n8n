# PostgreSQL
SERVICE_USER_POSTGRES=postgres
SERVICE_PASSWORD_POSTGRES=change_this_to_a_strong_password
POSTGRES_DB=chatwoot_production

# Redis
SERVICE_PASSWORD_REDIS=change_this_to_a_strong_password

# n8n
N8N_USER=admin
N8N_PASSWORD=change_this_to_a_strong_password
WEBHOOK_URL=http://host.docker.internal:5678
GENERIC_TIMEZONE=America/Sao_Paulo

# Chatwoot
SERVICE_PASSWORD_64_SECRETKEYBASE=change_this_to_a_strong_64_char_secret_key
MAILER_SENDER_EMAIL=support@yourdomain.com
SMTP_ADDRESS=smtp.yourprovider.com
SMTP_PORT=587
SMTP_USERNAME=your_smtp_username
SMTP_PASSWORD=your_smtp_password
FRONTEND_URL=http://localhost:3001
BAILEYS_PROVIDER_DEFAULT_CLIENT_NAME=Chatwoot
SERVICE_PASSWORD_64_BAILEYSDEFAULTAPIKEY=change_this_to_a_strong_64_char_api_key

# MinIO
MINIO_USER=minioadmin
MINIO_PORT_ROOT_PASSWORD=change_this_to_a_strong_password

# RabbitMQ
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=change_this_to_a_strong_password

# Grafana
GRAFANA_USER=admin
GRAFANA_PASSWORD=change_this_to_a_strong_password

# Baileys API
LOG_LEVEL=info
BAILEYS_LOG_LEVEL=error
